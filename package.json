{
  "name": "hawksee",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"vite --host\" \"npm run watch:css\"",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "compile:sass": "sass Sass/main.scss css/sassStyle.css",
    "sass": "sass --watch Sass/main.scss:css/style.css",
    "prefix:css": "postcss css/style.css --use autoprefixer -o css/style.prefix.css --watch",
    "compress:css": "sass css/style.prefix.css css/style.min.css --style=compressed",
    "concat:css": "npx concat -o css/style.concated.css css/icon-font.css css/style.min.css",
    "watch:css": "concurrently \"npm run sass\" \"npm run compile:sass\" \"npm run prefix:css\" \"npm run compress:css\" \"npm run concat:css\""
  },
  "dependencies": {
    "framer-motion": "^11.15.0",
    "lodash": "^4.17.21",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-countup": "^6.5.3",
    "react-dom": "^18.3.1",
    "react-icons": "^5.4.0",
    "react-intersection-observer": "^9.14.0",
    "react-responsive": "^10.0.0",
    "react-router-dom": "^7.1.3",
    "react-scroll-parallax": "^3.4.5",
    "react-scroll-trigger": "^0.6.14",
    "react-slick": "^0.30.3",
    "react-transition-group": "^4.4.5",
    "sass": "^1.83.0",
    "slick-carousel": "^1.8.1",
    "swiper": "^11.1.15"
  },
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "concat-cli": "^4.0.0",
    "concurrently": "^9.1.2",
    "eslint": "^9.15.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "globals": "^15.12.0",
    "postcss": "^8.5.2",
    "postcss-cli": "^11.0.0",
    "vite": "^6.0.1"
  }
}
